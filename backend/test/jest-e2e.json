{
  "rootDir": "../src",
  "moduleFileExtensions": ["js", "json", "ts"],
  "modulePathIgnorePatterns": ["<rootDir>/__mocks__"],
  "setupFiles": ["../test/setup-e2e-env.js"],
  "setupFilesAfterEnv": ["../test/matcher-extensions"],
  "testEnvironment": "node",
  "testRegex": ".e2e-spec.ts$",
  "testTimeout": 60000,
  "transform": {
    "^.+\\.(t|j)s$": "@swc/jest"
  }
}
