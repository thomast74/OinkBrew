{
  "name": "oinkbrew-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "export $(cat .env | xargs) && nest start",
    "start:dev": "export $(cat .env | xargs) && nest start --watch",
    "start:debug": "export $(cat .env | xargs) && nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "export $(cat .env | xargs) && jest",
    "test:watch": "export $(cat .env | xargs) && jest --watch",
    "test:cov": "export $(cat .env | xargs) && jest --coverage",
    "test:debug": "export $(cat .env | xargs) && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm-run-all --continue-on-error env:test e2e:up e2e:db:deploy e2e:run e2e:down",
    "env": "export $(cat .env | xargs)",
    "env:test": "export $(cat .env.test | xargs)",
    "e2e:up": "docker-compose -f ../docker-compose-e2e.yml up -d && sleep 3 ",
    "e2e:down": "docker-compose -f ../docker-compose-e2e.yml down",
    "e2e:run": "jest --ci --config ./test/jest-e2e.json --forceExit --detectOpenHandles",
    "e2e:db:deploy": "npx prisma migrate reset --force"
  },
  "dependencies": {
    "@nestjs/bull": "0.6.2",
    "@nestjs/common": "^9.2.0",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.2.0",
    "@nestjs/jwt": "^9.0.0",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^9.2.0",
    "@prisma/client": "4.6.1",
    "argon2": "0.30.2",
    "bull": "4.10.1",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "date-fns": "2.29.3",
    "otplib": "12.0.1",
    "particle-api-js": "9.4.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "qrcode": "1.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.7"
  },
  "devDependencies": {
    "@faker-js/faker": "7.6.0",
    "@nestjs/cli": "^9.1.5",
    "@nestjs/schematics": "^9.0.3",
    "@nestjs/testing": "^9.2.0",
    "@types/bull": "4.10.0",
    "@types/express": "^4.17.14",
    "@types/jest": "29.2.3",
    "@types/jest-when": "3.5.2",
    "@types/node": "^18.11.9",
    "@types/passport-jwt": "3.0.7",
    "@types/passport-local": "1.0.34",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "jest-mock-extended": "3.0.1",
    "jest-when": "3.5.2",
    "npm-run-all": "4.1.5",
    "prettier": "^2.7.1",
    "prettier-plugin-organize-imports": "3.2.0",
    "prisma": "4.6.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.1",
    "ts-jest": "^29.0.3",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "../test/matcher-extensions"
    ]
  }
}
