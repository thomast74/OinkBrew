{
  "name": "oinkbrew-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run export:env && nest start",
    "start:dev": "npm run export:env && nest start --watch",
    "start:debug": "npm run export:env && nest start --debug --watch",
    "start:prod": "node dist/main",
    "export:env": "export $(cat .env | grep -v '#' | grep -v -e '^$' | xargs)",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run export:env && jest",
    "test:watch": "npm run export:env && jest --watch",
    "test:cov": "npm run export:env && jest --coverage",
    "test:debug": "npm run export:env && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm-run-all --continue-on-error e2e:up e2e:run e2e:down",
    "e2e:up": "docker-compose -f ../docker-compose-e2e.yml up -d && sleep 20",
    "e2e:down": "docker-compose -f ../docker-compose-e2e.yml down --volumes",
    "e2e:run": "jest --ci --config ./test/jest-e2e.json --forceExit --detectOpenHandles --verbose"
  },
  "dependencies": {
    "@nestjs/bull": "10.0.0",
    "@nestjs/common": "10.0.2",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.0.2",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/mongoose": "10.0.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.0.2",
    "argon2": "0.30.3",
    "bull": "4.10.4",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.0",
    "date-fns": "2.30.0",
    "iterate": "0.1.1",
    "mongoose": "7.3.0",
    "otplib": "12.0.1",
    "particle-api-js": "9.4.1",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "qrcode": "1.5.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "5.0.1",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "8.0.2",
    "@nestjs/cli": "10.0.3",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.0.2",
    "@swc/cli": "0.1.62",
    "@swc/core": "1.3.66",
    "@swc/jest": "0.2.26",
    "@trivago/prettier-plugin-sort-imports": "4.1.1",
    "@types/bull": "4.10.0",
    "@types/express": "4.17.17",
    "@types/jest": "29.5.2",
    "@types/jest-when": "3.5.2",
    "@types/node": "^20.3.1",
    "@types/passport-jwt": "3.0.8",
    "@types/passport-local": "1.0.35",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.60.0",
    "@typescript-eslint/parser": "^5.60.0",
    "eslint": "^8.43.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.5.0",
    "jest-extended": "4.0.0",
    "jest-mock-extended": "3.0.4",
    "jest-when": "3.5.2",
    "mongodb-memory-server": "8.13.0",
    "npm-run-all": "4.1.5",
    "prettier": "^2.8.8",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "../test/matcher-extensions"
    ]
  }
}
