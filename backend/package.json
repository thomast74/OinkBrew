{
  "name": "oinkbrew-api",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run export:env && nest start",
    "start:dev": "npm run export:env && nest start --watch",
    "start:debug": "npm run export:env && nest start --debug --watch",
    "start:prod": "node dist/main",
    "export:env": "export $(cat .env | grep -v '#' | grep -v -e '^$' | xargs)",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "npm run export:env && jest",
    "test:watch": "npm run export:env && jest --watch",
    "test:cov": "npm run export:env && jest --coverage",
    "test:debug": "npm run export:env && node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "npm-run-all --continue-on-error e2e:up e2e:run e2e:down",
    "e2e:up": "docker-compose -f ../docker-compose-e2e.yml up -d && sleep 20",
    "e2e:down": "docker-compose -f ../docker-compose-e2e.yml down --volumes",
    "e2e:run": "jest --ci --config ./test/jest-e2e.json --forceExit --detectOpenHandles --verbose"
  },
  "dependencies": {
    "@nestjs/bull": "10.1.1",
    "@nestjs/common": "10.3.7",
    "@nestjs/config": "3.2.1",
    "@nestjs/core": "10.3.7",
    "@nestjs/jwt": "10.2.0",
    "@nestjs/mongoose": "10.0.4",
    "@nestjs/passport": "10.0.3",
    "@nestjs/platform-express": "10.3.7",
    "argon2": "0.40.1",
    "bull": "4.12.2",
    "class-transformer": "0.5.1",
    "class-validator": "0.14.1",
    "date-fns": "3.6.0",
    "eventsource": "2.0.2",
    "iterate": "0.1.1",
    "mongoose": "8.2.4",
    "otplib": "12.0.1",
    "particle-api-js": "10.4.3",
    "passport": "0.7.0",
    "passport-jwt": "4.0.1",
    "passport-local": "1.0.0",
    "qrcode": "1.5.3",
    "reflect-metadata": "0.2.2",
    "rimraf": "5.0.5",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@faker-js/faker": "8.4.1",
    "@nestjs/cli": "10.3.2",
    "@nestjs/schematics": "10.1.1",
    "@nestjs/testing": "10.3.7",
    "@swc/cli": "0.3.12",
    "@swc/core": "1.4.11",
    "@swc/jest": "0.2.36",
    "@trivago/prettier-plugin-sort-imports": "4.3.0",
    "@types/bull": "4.10.0",
    "@types/eventsource": "1.1.15",
    "@types/express": "4.17.21",
    "@types/jest": "29.5.12",
    "@types/jest-when": "3.5.5",
    "@types/node": "^20.12.2",
    "@types/passport-jwt": "4.0.1",
    "@types/passport-local": "1.0.38",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "jest": "^29.7.0",
    "jest-extended": "4.0.2",
    "jest-mock-extended": "3.0.5",
    "jest-when": "3.6.0",
    "mongodb-memory-server": "9.1.8",
    "npm-run-all": "4.1.5",
    "prettier": "^3.2.5",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.1.2",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "@swc/jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "../test/matcher-extensions"
    ]
  }
}
